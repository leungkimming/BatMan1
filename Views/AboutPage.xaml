<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="BatMan2.Views.AboutPage" BackgroundColor="White">
    <ContentPage.Content>
        <StackLayout VerticalOptions="FillAndExpand">
            <Image HeightRequest="70" Source="batman.png"></Image>
            <WebView IsVisible="true" x:Name="proteoWebView" 
                     VerticalOptions="FillAndExpand"
                     HorizontalOptions="FillAndExpand" Source="about.html">
            </WebView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
